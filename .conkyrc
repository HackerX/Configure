background yes
imlib_cache_size 0
use_xft yes
#xftfont Droid Sans:size=9
xftfont wqy-zenhei:size=10
xftalpha 0.9
update_interval 1.0
total_run_times 0
own_window yes
own_window_type override
own_window_transparent yes
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
double_buffer yes
#minimum_size 220 5
#maximum_width 150
use_spacer right
draw_shades no
draw_outline no
draw_borders no
draw_graph_borders yes
#stippled_borders 8
# border_margin 0
# border_width 1
default_color FFFFFF
color1 1793D1
color2 535353
color3 FFFFFF
color4 D9D9D9
color5 1793D1
color6 red
#alignment top_middle
#alignment top_left
#alignment top_right
#alignment bottom_left
alignment bottom_right
#alignment none
gap_x 0
#gap_y 45
gap_y 0
no_buffers yes
uppercase no
cpu_avg_samples 2
override_utf8_locale yes
default_bar_size 45 20

#${color3}CPU: ${color3}${cpu cpu0}% ${color5} | \
#${color3}Home: ${color3}${fs_free_perc /home}% ${color4}free ${color5} | \
